{
  "name": "storybook-framework-cedarjs",
  "version": "0.10.0",
  "description": "Storybook for CedarJS",
  "keywords": [
    "Storybook",
    "Cedar",
    "React",
    "Vite"
  ],
  "bugs": {
    "url": "git+https://github.com/cedarjs/cedar/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedarjs/cedar.git",
    "directory": "packages/storybook"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "default": "./dist/preset.js"
    },
    "./dist/mocks/MockRouter": {
      "default": "./dist/mocks/MockRouter.js"
    },
    "./dist/preview.js": "./dist/preview.js",
    "./package.json": "./package.json"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "tsx build.ts && yarn build:types",
    "build:pack": "yarn pack -o storybook-framework-cedarjs.tgz",
    "build:types": "tsc --build --verbose",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@cedarjs/testing": "workspace:*",
    "@joshwooding/vite-plugin-react-docgen-typescript": "0.6.2",
    "@rollup/pluginutils": "5.1.4",
    "@storybook/addon-essentials": "7.6.20",
    "@storybook/builder-vite": "7.6.20",
    "@storybook/react": "7.6.20",
    "magic-string": "0.30.18",
    "react-docgen": "7.0.3",
    "unplugin-auto-import": "19.3.0",
    "vite-plugin-node-polyfills": "0.23.0"
  },
  "devDependencies": {
    "@types/node": "22.18.12",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "vite": "5.4.19",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@cedarjs/project-config": "workspace:*",
    "@cedarjs/router": "workspace:*",
    "react": "19.0.0-rc-f2df5694-20240916",
    "react-dom": "19.0.0-rc-f2df5694-20240916",
    "storybook": "7.6.20"
  },
  "engines": {
    "node": "=22.21.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/preset.ts"
    ],
    "platform": "node"
  }
}
