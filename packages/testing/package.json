{
  "name": "@cedarjs/testing",
  "version": "0.10.0",
  "description": "Tools, wrappers and configuration for testing a Cedar project.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedarjs/cedar.git",
    "directory": "packages/testing"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./config/jest/web": {
      "default": "./config/jest/web/index.js"
    },
    "./config/jest/api": {
      "default": "./config/jest/api/index.js"
    },
    "./config/jest/api/jest-preset": {
      "default": "./config/jest/api/jest-preset.js"
    },
    "./config/jest/web/jest-preset": {
      "default": "./config/jest/web/jest-preset.js"
    },
    "./dist/cjs/api/scenario.js": {
      "default": "./dist/cjs/api/scenario.js"
    },
    "./dist/cjs/api/directUrlHelpers.js": {
      "default": "./dist/cjs/api/directUrlHelpers.js"
    },
    "./dist/cjs/web/findCellMocks": {
      "default": "./dist/cjs/web/findCellMocks.js"
    },
    "./dist/cjs/web/findCellMocks.js": {
      "default": "./dist/cjs/web/findCellMocks.js"
    },
    "./dist/cjs/web/mockRequests": {
      "default": "./dist/cjs/web/mockRequests.js"
    },
    "./dist/cjs/web/mockRequests.js": {
      "default": "./dist/cjs/web/mockRequests.js"
    },
    "./dist/cjs/web/fileMock.js": {
      "default": "./dist/cjs/web/fileMock.js"
    },
    "./auth": {
      "import": {
        "types": "./dist/web/mockAuth.d.ts",
        "default": "./dist/web/mockAuth.js"
      },
      "require": {
        "types": "./dist/cjs/web/index.d.ts",
        "default": "./dist/cjs/web/mockAuth.js"
      }
    },
    "./api": {
      "import": {
        "types": "./dist/api/index.d.ts",
        "default": "./dist/api/index.js"
      },
      "require": {
        "types": "./dist/cjs/api/index.d.ts",
        "default": "./dist/cjs/api/index.js"
      }
    },
    "./api/vitest": {
      "types": "./dist/api/vitest/index.d.ts",
      "default": "./dist/api/vitest/index.js"
    },
    "./cache": {
      "import": {
        "types": "./dist/cache/index.d.ts",
        "default": "./dist/cache/index.js"
      },
      "require": {
        "types": "./dist/cjs/cache/index.d.ts",
        "default": "./dist/cjs/cache/index.js"
      }
    },
    "./web": {
      "import": {
        "types": "./dist/web/index.d.ts",
        "default": "./dist/web/index.js"
      },
      "require": {
        "types": "./dist/cjs/web/index.d.ts",
        "default": "./dist/cjs/web/index.js"
      }
    },
    "./web/MockRouter.js": {
      "import": {
        "types": "./dist/web/MockRouter.d.ts",
        "default": "./dist/web/MockRouter.js"
      },
      "require": {
        "types": "./dist/cjs/web/MockRouter.d.ts",
        "default": "./dist/cjs/web/MockRouter.js"
      }
    },
    "./web/vitest": {
      "types": "./dist/web/vitest/index.d.ts",
      "default": "./dist/web/vitest/index.js"
    }
  },
  "files": [
    "config",
    "dist"
  ],
  "scripts": {
    "build": "tsx ./build.mts && yarn build:types && ls -la",
    "build:pack": "ls -la && yarn pack -o cedarjs-testing.tgz",
    "build:types": "tsc --build --verbose ./tsconfig.build.json",
    "build:types-cjs": "tsc --build --verbose ./tsconfig.cjs.json",
    "build:watch": "nodemon --watch src --ext 'js,jsx,ts,tsx' --ignore dist --exec 'yarn build'",
    "cache:analyze": "tsx ./analyze-cache-reports.mts",
    "cache:analyze:detailed": "tsx ./analyze-cache-reports.mts detailed",
    "cache:analyze:history": "tsx ./analyze-cache-reports.mts history",
    "check:attw": "yarn rw-fwtools-attw",
    "check:package": "concurrently npm:check:attw yarn:publint",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@cedarjs/auth": "workspace:*",
    "@cedarjs/babel-config": "workspace:*",
    "@cedarjs/context": "workspace:*",
    "@cedarjs/graphql-server": "workspace:*",
    "@cedarjs/project-config": "workspace:*",
    "@cedarjs/router": "workspace:*",
    "@cedarjs/web": "workspace:*",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.5.2",
    "@types/aws-lambda": "8.10.152",
    "@types/babel-core": "6.25.10",
    "@types/jest": "29.5.14",
    "@types/node": "22.18.12",
    "ansis": "4.1.0",
    "babel-jest": "^29.7.0",
    "fast-glob": "3.3.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "msw": "1.3.5",
    "ts-toolbelt": "9.6.0",
    "unplugin-auto-import": "19.3.0",
    "whatwg-fetch": "3.6.20"
  },
  "devDependencies": {
    "@cedarjs/framework-tools": "workspace:*",
    "concurrently": "8.2.2",
    "jsdom": "24.1.3",
    "publint": "0.3.12",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "vitest": "3.2.4"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1",
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/dist",
          "{projectRoot}/config"
        ]
      }
    }
  }
}
